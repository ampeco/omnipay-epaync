{
    "name": "ampeco/omnipay-epaync",
    "description": "Omnipay plugin for: https://epaync.nc/doc/fr-FR/rest/V4.0/api/ and https://payzen.io/en-EN/rest/V4.0/api/",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "autoload": {
        "psr-4": { "Omnipay\\EpayNC\\": "src/" }
    },
    "authors": [
        {
            "name": "Alexander Alexiev",
            "email": "alexander.alexiev@ampeco.com"
        },
        {
            "name": "Petko Kostov",
            "email": "petko.kostov@ampeco.global"
        },
        {
            "name": "Marry Akifova",
            "email": "merry.akifova@ampeco.global"
        }
    ],
    "require": {
        "omnipay/common": "^3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
